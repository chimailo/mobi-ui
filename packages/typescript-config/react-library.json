{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"allowJs": true,
		"verbatimModuleSyntax": true,
		"noImplicitOverride": true,
		"module": "preserve",
		"noEmit": true,
		"declaration": false,
		"declarationMap": false,
		"jsx": "react-jsx"
	}
}
